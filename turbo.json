{
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**"]
    },
    "lint": {
      "outputs": []
    },
    "dev": {
      "cache": false
    },
    "storybook": {
      "cache": false
    },
    "build:storybook": {
      "outputs": ["storybook-static/**"]
    },
    "build:ui": {
      "cache": false
    },
    "build:tokens": {
      "cache": false
    },
    "test": {
      "cache": false
    }
  }
}
