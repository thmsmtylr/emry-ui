{
  "name": "@emryui/react-button",
  "version": "0.0.7-alpha",
  "license": "MIT",
  "description": "Emry UI React button component",
  "author": "Emry UI <emryui@proton.me>",
  "homepage": "https://emryui.com",
  "source": "./src/index.ts",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/",
    "directory": "./dist"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "sideEffects": false,
  "scripts": {
    "clean": "rm -rf dist",
    "version": "yarn version",
    "build": "npm run clean && parcel build src/index.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thmsmtylr/emry-ui.git"
  },
  "tsconfig": "./tsconfig.json",
  "peerDependencies": {
    "@types/react": "*",
    "@types/react-dom": "*",
    "react": "^16.8 || ^17.0 || ^18.0",
    "react-dom": "^16.8 || ^17.0 || ^18.0"
  },
  "peerDependenciesMeta": {
    "@types/react": {
      "optional": true
    },
    "@types/react-dom": {
      "optional": true
    }
  },
  "keywords": [
    "Emry UI",
    "Emry UI button",
    "Tailwind",
    "css"
  ]
}
